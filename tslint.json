{
  "extends": "tslint-config-airbnb",
  "rules": {
    "no-unused-expression": true,
    "object-shorthand-properties-first": false,
    "max-line-length": {
      "options": [120]
    },
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
