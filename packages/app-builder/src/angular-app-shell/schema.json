{
  "$schema": "http://json-schema.org/schema",
  "id": "angularAppShell",
  "title": "Generates the angular-app-shell for the @sketchmine/app-builder",
  "type": "object",
  "properties": {
    "config": {
      "type": "string",
      "format": "path",
      "description": "Path to the config.json",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "dependencies": {
      "type": "string",
      "description": "Additional dependencies or version overwrites"
    }
  },
  "required": [
    "config"
  ]
}
